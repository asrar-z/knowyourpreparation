<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="k.Yp_Original"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="k.Yp_Original.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="k.Yp_Original.SelectionActivity"
            android:label="@string/title_activity_selection"
            android:parentActivityName="k.Yp_Original.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="k.Yp_Original.MainActivity" />
        </activity>
        <activity
            android:name="k.Yp_Original.QDisp"
            android:label="@string/title_activity_capps"
            android:parentActivityName="k.Yp_Original.SelectionActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="k.Yp_Original.SelectionActivity" />
        </activity>

        <activity
            android:name="k.Yp_Original.Res"
            android:label="@string/title_activity_res" >
        </activity>
        <activity
            android:name="k.Yp_Original.Test"
            android:label="@string/title_activity_test"
            android:parentActivityName="k.Yp_Original.SelectionActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="k.Yp_Original.SelectionActivity" />
        </activity>
    </application>

</manifest>
